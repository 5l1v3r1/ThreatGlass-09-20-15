function changeMenu_LA(){
        if("" == ""){
        document.getElementById("topmenu_01").style.display = "block";
        document.getElementById("tab01").className = "naviON";
    }else if("" != ""){
        if("" == "92"){
            document.getElementById("topmenu_01").style.display = "block";
            document.getElementById("tab01").className = "naviON";
        }else if("" == "93"){
            document.getElementById("topmenu_02").style.display = "block";
            document.getElementById("tab02").className = "naviON";
        }else if("" == "autos"){
            document.getElementById("topmenu_03").style.display = "block";
            document.getElementById("tab03").className = "naviON";
        }else if("" == "96"){
			if("" == "218"){
            document.getElementById("topmenu_04").style.display = "block";
            document.getElementById("tab04").className = "naviON";
			}else{
            document.getElementById("topmenu_05").style.display = "block";
            document.getElementById("tab05").className = "naviON";
			}
        }else if("" == "94"){
            document.getElementById("topmenu_04").style.display = "block";
            document.getElementById("tab04").className = "naviON";
        }else if("" == "99"){
            document.getElementById("topmenu_05").style.display = "block";
            document.getElementById("tab05").className = "naviON";
        }else if("" == "97"){
            document.getElementById("topmenu_06").style.display = "block";
            document.getElementById("tab06").className = "naviON";
            
        }else if("" == "109"){
            document.getElementById("topmenu_07").style.display = "block";
            document.getElementById("tab07").className = "naviON";
            
        }else if("" == "126"){
            document.getElementById("topmenu_08").style.display = "block";
            document.getElementById("tab08").className = "naviON";
            
        }else if("" == "110"){
            document.getElementById("topmenu_09").style.display = "block";
            document.getElementById("tab09").className = "naviON";
        }else{
            document.getElementById("topmenu_01").style.display = "block";
            document.getElementById("tab01").className = "naviON";
        }
    }
}
  
        function func_MenuMore(){
            if(document.getElementById("popMoreMenu").style.display == "none"){
                document.getElementById("popMoreMenu").style.display = "block";
            }else{
                document.getElementById("popMoreMenu").style.display = "none";
            }
        }


